        .button {
            margin: 0;
            height: auto;
            background: transparent;
            padding: 0;
            border: none;
            cursor: pointer;
        }

        /* button styling */
        .button {
            position: relative;
            top: 20%;
            left: 50%;
            transform: translate(-50%, 10%);
            --border-right: 0px;
            --text-stroke-color: rgb(0 38 255);
            --animation-color: #0d23a1;
            --fs-size: 9em;
            letter-spacing: 0px;
            text-decoration: none;
            font-size: var(--fs-size);
            font-family: "Cascadia Code";
            position: relative;
            text-transform: uppercase;
            color: transparent;
            -webkit-text-stroke: 1px var(--text-stroke-color);
        }
        /* this is the text, when you hover on button */
        .hover-text {
            position: absolute;
            box-sizing: border-box;
            content: attr(data-text);
            color: var(--animation-color);
            width: 0%;
            inset: 0;
            border-right: var(--border-right) solid var(--animation-color);
            overflow: hidden;
            transition: 0.8s;
            -webkit-text-stroke: 1px var(--animation-color);
        }
        /* hover */
        .button:hover .hover-text {
            width: 100%;
            filter: drop-shadow(0 0 23px var(--animation-color))
        }

        .fecha-hora {
            font-family: Arial, sans-serif;
            font-size: 18px;
            color: #000;
        }

        .fecha {
            font-weight: bold;
        }

        .hora {
            color: #000;
            font-size: 20px;
        }

        .form-control {
            outline: none;
            font-size: 16px;
            border-radius: 5px;
            padding-left: 15px;
            border: 1px solid #00ff2a;
            border-bottom-width: 2px;
            transition: all 0.3s ease;
            position: relative;
        }

            .form-control:valid {
                border-color: rgb(0 38 255);
                box-shadow: 0.2px 0.2px 1px -10px #00ff2a;
            }

            .form-control:invalid {
                border-color: rgb(255 0 0);
                color: rgb(255 0 0);
                box-shadow: 1px 1px 1px -10px #000000;
                /* translate: -8px 8px 0px 0px; */
            }

        .text {
            margin-top: 10px;
            color: black;
        }


        .loader {
            position: relative;
            pointer-events: none;
        }

            .loader span {
                position: absolute;
                color: #fff;
                font-size: 100px;
                font-family: "Leelawadee UI";
                letter-spacing: 0px;
            }

                .loader span:nth-child(1) {
                    color: transparent;
                    -webkit-text-stroke: 1px rgb(0 38 255);
                }

                .loader span:nth-child(2) {
                    color: #0d23a1;
                    -webkit-text-stroke: 1px rgb(0 0 0);
                    animation: uiverse723 4s ease-in-out infinite;
                }

        @@keyframes uiverse723 {
            0%, 100% {
                clip-path: polygon(0% 45%, 15% 44%, 32% 50%, 54% 60%, 70% 61%, 84% 59%, 100% 52%, 100% 100%, 0% 100%);
            }

            50% {
                clip-path: polygon(0% 60%, 16% 65%, 34% 66%, 51% 62%, 67% 50%, 84% 45%, 100% 46%, 100% 100%, 0% 100%);
            }
        }


        .text {
            margin-top: 10px;
            color: black;
        }


